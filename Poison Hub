    PremiumOnly = false
})

BloxFruitsTab:AddDropdown({
    Name = "HoHo Hub",
    Default = "Selecione",
    Options = {"Selecione", "HoHo Hub Main", "HoHo Hub Kaitun", "HoHo Auto Bounty"},
    Callback = function(Value)
        if Value == "HoHo Hub Main" then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI"))()
        elseif Value == "HoHo Hub Kaitun" then
            loadstring(game:HttpGet("https://pastebin.com/raw/vFV6gGfJ"))()
        elseif Value == "HoHo Auto Bounty" then
            loadstring(game:HttpGet("https://pastebin.com/raw/pajZ9VHR"))()
        else
            print("Por favor, selecione uma opção válida.")
        end
    end    
})

