    Name = "Bem-vindo",
    Content = "Bem-vindo ao Poison Hub " .. game.Players.LocalPlayer.Name .. "!",
    Image = "rbxassetid://4483345998", -- Imagem opcional, pode ser removida ou alterada
    Time = 5
})

