-- Verifica se o ScreenGui já existe e o remove
local existingGui = game.CoreGui:FindFirstChild("MovableButtonGui")
if existingGui then
    existingGui:Destroy()
end

-- Criação do botão fora da GUI principal
local ScreenGui = Instance.new("ScreenGui")
local ToggleButton = Instance.new("TextButton")
local UICorner = Instance.new("UICorner")
local ImageLabel = Instance.new("ImageLabel")
local ImageUICorner = Instance.new("UICorner")

-- Nomeia a ScreenGui para evitar conflitos
ScreenGui.Name = "MovableButtonGui"
ScreenGui.Parent = game.CoreGui

-- Configuração do botão
ToggleButton.Name = "MovableButton"
ToggleButton.Parent = ScreenGui
ToggleButton.Size = UDim2.new(0, 35, 0, 35) -- Tamanho reduzido para 35x35 pixels
ToggleButton.Position = UDim2.new(1, -70, 0, 10)  -- Ajuste a posição para o topo direito da tela, movendo 10 pixels para a esquerda
ToggleButton.BackgroundTransparency = 1  -- Torna o botão transparente
ToggleButton.BorderSizePixel = 0

-- Configuração da imagem
ImageLabel.Name = "ImageLabel"
ImageLabel.Parent = ToggleButton
ImageLabel.Size = UDim2.new(1, 0, 1, 0)  -- Tamanho igual ao do botão
ImageLabel.Position = UDim2.new(0, 0, 0, 0)
ImageLabel.BackgroundTransparency = 1  -- Torna o fundo da imagem transparente
ImageLabel.Image = "rbxassetid://9508254002"  -- ID da imagem fornecida

-- Adiciona bordas arredondadas para fazer a imagem circular
ImageUICorner.Parent = ImageLabel
ImageUICorner.CornerRadius = UDim.new(1, 0)  -- Faz a imagem completamente circular

-- Adiciona bordas arredondadas para o botão
UICorner.Parent = ToggleButton
UICorner.CornerRadius = UDim.new(1, 0)  -- Faz o botão completamente circular

-- Função para alternar a visibilidade da GUI
local function toggleGUI()
    local orionGUI = game.CoreGui:FindFirstChild("Orion")
    if orionGUI then
        orionGUI.Enabled = not orionGUI.Enabled
    else
        warn("A GUI chamada 'Orion' não foi encontrada no CoreGui.")
    end
end

-- Conecta a função de toggle ao botão
ToggleButton.MouseButton1Click:Connect(toggleGUI)

-- Função para arrastar o botão
local UIS = game:GetService("UserInputService")
local dragging, dragInput, dragStart, startPos

local function update(input)
    local delta = input.Position - dragStart
    ToggleButton.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

ToggleButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = ToggleButton.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

ToggleButton.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UIS.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        update(input)
    end
end)

-- Importa a biblioteca Orion
local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()

